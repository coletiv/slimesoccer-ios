# SlimeSoccer will be developed for iOS 11.0+
platform :ios, '11.0'

# Workspace
workspace 'SlimeSoccer'

# Project
project './SlimeSoccer/SlimeSoccer.xcodeproj'

# Use frameworks instead of static libraries for Pods
use_frameworks!
# We don't want to receive warnings from Pods
inhibit_all_warnings!

# SlimeSoccer Target
target 'SlimeSoccer' do
  
  # Socket Framework
  pod 'SwiftPhoenixClient'

end
